#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules/ember-template-lint/bin/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules/ember-template-lint/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules/ember-template-lint/bin/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules/ember-template-lint/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/ember-template-lint@6.0.0/node_modules:/Users/robertbarrow/dev/disc/code/discourse-tc-sub-redirect/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ember-template-lint.js" "$@"
else
  exec node  "$basedir/../../bin/ember-template-lint.js" "$@"
fi
